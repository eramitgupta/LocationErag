{
    "name": "erag/locationerag",
    "description": "A simple package to get location data using Indian PIN codes, optimized for Laravel 10.x and 11.x",
    "license": "MIT",
    "keywords": [
        "laravel",
        "location",
        "india",
        "pincode",
        "geolocation",
        "location finder",
        "indian pincode location"
    ],
    "authors": [
        {
            "name": "Er Amit Gupta",
            "email": "info.eramitgupta@gmail.com"
        }
    ],
    "homepage": "https://github.com/eramitgupta/locationerag",
    "require": {
        "php": ">=8.0.0"
    },
    "autoload": {
        "psr-4": {
            "LocationErag\\": "src/"
        }
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "LocationErag\\LocationEragServiceProvider"
            ]
        }
    }
}
